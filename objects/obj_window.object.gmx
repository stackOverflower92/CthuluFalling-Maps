            <string>///Create Event
my_string = "initial";
default_text = "initial";
new_string = "";
save_string = my_string;
typed_letters = 0;
can_type = true;
can_draw = true;
string_x = x//200;
string_y = y//200;
pages = 1;
current_page = 1;
sharp = "#";
max_lines = 6;
current_line = 0;
chars_drawn = 1;
line_len = 47
sharp_n = 0;
lines_per_page = 6;
end_page_pressed = false;
border_bool = true;
rect_w = 430; rect_h = 100;
triggered = false;
gap = 0;
lines_drawn = 0;
type_fast = false;
blocks_letters = 0;
wait_user_input = false;
gap_add = 0.3;

//insert new lines
for (i = 0; i &lt; string_length(my_string); i++)
    if ((i % line_len) == 0 &amp;&amp; i != 0)
        my_string = string_insert(sharp, my_string, i)
        
//counting pages
for (i = 0; i &lt; string_length(my_string); i++){
    if (string_char_at(my_string, i) == '#'){
        sharp_n++;
    }    
}

pages = ceil(sharp_n / lines_per_page);
