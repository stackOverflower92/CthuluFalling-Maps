          <argument>
            <kind>1</kind>
            <string>///Draw Event
var begin_count = 0;
var VIEWPORT_WIDTH = window_get_width();
var VIEWPORT_HEIGHT = window_get_height();
var DEVICE_SCREEN_WIDTH = display_get_width();
var DEVICE_SCREEN_HEIGHT = display_get_height();
var VIEWPORT_X = view_xview[0];
var VIEWPORT_Y = view_yview[0];

//Rect 1
var p1x = VIEWPORT_X + string_x - 15;
var p1y = VIEWPORT_Y + string_y
var p2x = p1x + rect_w;
var p2y = p1y + rect_h;

//Rect 2
var p1x_2 = p1x - 1;
var p1y_2 = p1y - 1;
var p2x_2 = p2x + 1;
var p2y_2 = p2y + 1;

//if (keyboard_check(vk_alt)) triggered = true;

if (default_text != my_string){
    //insert new lines
    for (i = 0; i &lt; string_length(my_string); i++)
        if ((i % line_len) == 0 &amp;&amp; i != 0)
            my_string = string_insert(sharp, my_string, i)
        
    //counting pages
    for (i = 0; i &lt; string_length(my_string); i++){
        if (string_char_at(my_string, i) == '#'){
            sharp_n++;
        }
    }    

    //delete spaces
    for (i = 0; i &lt; string_length(my_string); i++){
        if (string_char_at(my_string, i) == '#' and i &lt; string_length(my_string) - 1 and i &gt; 0){
            if (string_char_at(my_string, i - 1) == ' ') string_delete(my_string, i - 1, 1);
            if (string_char_at(my_string, i + 1) == ' ') string_delete(my_string, i + 1, 1);
        }
    }
    
    pages = ceil(sharp_n / lines_per_page);

    triggered = true;
    can_type = true;
    can_draw = true;
    typed_letters = 0;
    default_text = my_string;
}

if (triggered == true){
    if (can_type == true || can_draw == true){
        draw_rectangle_color(p1x, p1y, p2x, p2y, c_gray, c_gray, c_dkgray, c_dkgray, false);
        draw_rectangle_color(p1x_2, p1y_2, p2x_2, p2y_2, c_maroon, c_maroon, c_maroon, c_maroon, true);
    }
    
    if (typed_letters &lt; string_length(my_string)){
        if (type_fast == false){
            gap_add = 0.3; 
            typed_letters += gap_add;
        }
        else{
            gap_add = 1;
            typed_letters += gap_add;
            blocks_letters++;
        }
    }
    
    if (ceil(typed_letters) == string_length(my_string)){
        can_type = false;
    }
    
    if (current_line == max_lines){ 
        if (current_page == pages){
            can_type = false;
            
            if (end_page_pressed == false){
                show_message("end of all");
            
                type_fast = false;
                end_page_pressed = true;
                can_draw = false;
                triggered = false;
                exit;
            }
                
            typed_letters = 0;
        }
        else{
            //show_message("next page: enter");
            wait_user_input = true;
            
            if (type_fast == true) type_fast = false;
              
            current_page++;
            can_type = false;       
            current_line = 0;    
            new_string = string_copy(my_string, ceil(typed_letters) + 1, string_length(my_string) - ceil(typed_letters));
            typed_letters = 0;
            my_string = new_string
            new_string = ""
            can_type = true;
            gap = 0;
            chars_drawn = 1;
        }
    }
    
    if (can_type == true){
        construct_string = string_copy(my_string, 0, ceil(typed_letters));
        draw_text(p1x + 4, p1y + 4, construct_string);
    }
    else{
        draw_text(p1x + 4, p1y + 4, construct_string);
        can_draw = true;
    }
    
    if ((floor(typed_letters) == chars_drawn + 1 + gap)){ chars_drawn++; audio_play_sound(type_sound, 10, false); }
      
    if (chars_drawn == line_len){current_line++; chars_drawn = 1; gap += line_len}
    
    if (keyboard_check(vk_f1)){
        type_fast = true;
    }
    
    if (keyboard_check(vk_f2)){
        can_type = true;
    }
    
    if (wait_user_input == true){
        gap_add = 0;
    }   
}
</string>
          </argument>
        </arguments>
      </action>
